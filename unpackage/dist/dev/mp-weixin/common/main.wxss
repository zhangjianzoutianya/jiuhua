














/*每个页面公共css */
page{
	width: 100%;
	height: 100%;
}


.business .pages{
	padding-top: 45rpx;
	padding-bottom: 100rpx;
}
.business .business_list{
	padding: 60rpx 27rpx 40rpx 27rpx;
	border-bottom: 20rpx solid rgba(247,247,247,1);
}
.business .business_h5{
	position: relative;
	font-size:32rpx;
	font-weight:500;
	color:rgba(51,51,51,1);
	line-height:24rpx;
	padding-bottom: 20rpx;
	margin-bottom: 35rpx;
}
.business .business_h5:before{
	content: '';
	position: absolute;
	bottom: 0;
	left: 0;
	width:32rpx;
	height:4rpx;
	background-color: rgba(201,0,9,1);
}
.business .business_h5 .txt{
	height:24rpx;
	font-size:24rpx;
	color:rgba(201,0,9,1);
	line-height:24rpx;
	position: absolute;
	top: 0;
	right: 0;
}
.business .business_h5 .txt image{
	display: inline-block;
	width:12rpx;
	height:22rpx;
	margin-left: 10rpx;
}
.business .business_list_1 .business_list_banner{
	width: 100%;
}
.business .business_list_1 .business_list_banner image{
	width: 695rpx;
	height: 345rpx;
}
.business .unmanned {
	width: 100%;
	height: 150rpx;
	overflow: hidden;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	margin-top: 46rpx;
}
.business .unmanned-view {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	height: 100%;
}
.business .unmanned-img {
	width: 109rpx;
	height: 109rpx;
	margin: 0 auto;
	overflow: hidden;
}
.business .unmanned-img>image {
	width: 100%;
	height: 100%;
}
.business .unmanned-text {
	text-align: center;
	height: 28rpx;
	font-size: 28rpx;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
}
.business .sell-like-hot-cakes {
	margin: 0 auto;
	margin-top: 53rpx;
}
.business .sell-like-hot-cakes-text {
	height: 32rpx;
	font-size: 30rpx;
	font-weight: bold;
	color: rgba(51, 51, 51, 1);
	line-height: 32rpx;
}
.business .sell-like-hot-cakes-i {
	width: 35rpx;
	height: 4rpx;
	margin-top: 8rpx;
	background: rgba(201, 201, 201, 1);
}
.business .area {
	margin-top: 32rpx;
	width: 698rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.business .area-view {
	width: 226rpx;
	height: 292rpx;
	border-radius: 14rpx;
	position: relative;
}
.business .area-view:nth-child(2) {
	margin: 0 10rpx;
}
.business .area-view>image {
	width: 100%;
	height: 100%;
}
.business .mask {
	position: absolute;
	bottom: 15rpx;
	left: 0;
	padding:0 19rpx;
}
.business .ranking {
	width: 99rpx;
	height: 32rpx;
	background: -webkit-gradient(linear, left bottom, left top, from(rgba(232, 129, 54, 1)), to(rgba(232, 70, 53, 1)));
	background: -o-linear-gradient(bottom, rgba(232, 129, 54, 1) 0%, rgba(232, 70, 53, 1) 100%);
	background: linear-gradient(0deg, rgba(232, 129, 54, 1) 0%, rgba(232, 70, 53, 1) 100%);
	opacity: 0.8;
	border-radius: 32rpx 0 32rpx 0;
	font-size: 26rpx;
	color: rgba(255, 255, 255, 1);
	line-height: 32rpx;
	padding-left: 10rpx;
}
.business .area-text {
	height: 27rpx;
	font-size: 26rpx;
	color: rgba(255, 255, 255, 1);
	line-height: 27rpx;
	margin-top: 50rpx;
}
.business .area-name {
	height: 27rpx;
	font-size: 24rpx;
	color: rgba(255, 255, 255, 1);
	line-height: 27rpx;
	margin-top: 19rpx;
}
.business .business_list_2 .business_list_banner image{
	width: 698rpx;
	height: 305rpx;
}
.business .business_list_3 .business_list_banner image{
	width: 698rpx;
	height: 358rpx;
}
.business .business_list_4 .business_list_banner image{
	width: 698rpx;
	height: 357rpx;
}
.business .business_list_5{
	padding: 60rpx 0rpx 40rpx 0rpx;
}
.business .business_list_5 .business_list_banner image{
	width: 750rpx;
	height: 412rpx;
}
.business .business_list_5 .business_h5{
	padding-left: 27rpx;
	padding-right: 27rpx;
}
.business .business_list_5 .business_h5:before{
	left: 27rpx;
}
.business .business_list_5 .business_h5 .txt{
	right: 27rpx;
}
.business .business_list_6 .business_list_banner image{
	width: 698rpx;
	height: 358rpx;
}





.user .pages {
	background-color: #F7F7F7;
	overflow: hidden;
	width: 100%;
	height: 100%;
}
.user .nav-view {
	width: 750rpx;
	height: 454rpx;
	overflow: hidden;
	position: relative;
}
.user .nav-bg {
	width: 100%;
	height: 366rpx;
	overflow: hidden;
}
.user .nav-bg>image {
	width: 100%;
	height: 100%;
	position: relative;
}
.user .information {
	position: absolute;
	width: 100%;
	height: 130rpx;
	top: 129rpx;
	left: 0;
	overflow: hidden;
}
.user .sign-in {
	width: 119rpx;
	height: 66rpx;
	background: rgba(255, 255, 255, 0.3);
	opacity: 0.25;
	border-radius: 33rpx 0 0 33rpx;
	font-size: 30rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
	line-height: 66rpx;
	opacity: 0.8;
	float: right;
	text-align: center;
}
.user .logo-img {
	width: 130rpx;
	height: 130rpx;
	border-radius: 65rpx;
	margin-left: 31rpx;
	margin-right: 53rpx;
	float: left;
	overflow: hidden;
}
.user .logo-img>image {
	width: 100%;
	height: 100%;
}
.user .name-view {
	width: 400rpx;
	height: 130rpx;
	overflow: hidden;
}
.user .name-view1 {
	width: 400rpx;
	height: 130rpx;
	overflow: hidden;
	line-height: 130rpx;
	font-size: 32rpx;
	font-family: PingFang-SC-Bold;
	font-weight: bold;
	color: rgba(255, 255, 255, 1);
}
.user .name-view1>text:nth-child(1) {
	padding-right: 10rpx;
}
.user .name-view1>text:nth-child(2) {
	padding-left: 10rpx;
}
.user .name {
	width: 100%;
	height: 74rpx;
	line-height: 74rpx;
	font-size: 32rpx;
	font-family: PingFang-SC-Bold;
	font-weight: bold;
	color: rgba(255, 255, 255, 1);
}
.user .name-img {
	width: 97rpx;
	height: 36rpx;
	overflow: hidden;
}
.user .name-img>image {
	width: 100%;
	height: 100%;
}
.user .balance {
	position: absolute;
	width: 698rpx;
	height: 154rpx;
	bottom: 0;
	margin-left: 27rpx;
	background: rgba(255, 255, 255, 1);
	-webkit-box-shadow: 0rpx 0rpx 57rpx 0rpx rgba(110, 110, 110, 0.1);
	        box-shadow: 0rpx 0rpx 57rpx 0rpx rgba(110, 110, 110, 0.1);
	border-radius: 10rpx;
	overflow: hidden;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.user .info {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	    -ms-flex: 1;
	        flex: 1;
	width: 100%;
	height: 100%;
}
.user .info>view {
	text-align: center;
}
.user .info>view:nth-child(1) {
	font-size: 30rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
	margin-top: 35rpx;
}
.user .info>view:nth-child(2) {
	font-size: 24rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
	margin-top: 15rpx;
}
.user .paging {
	width: 750rpx;
	height: 88rpx;
	background: rgba(255, 255, 255, 1);
	margin-top: 20rpx;
	overflow: hidden;
}
.user .more {
	width: 18rpx;
	height: 30rpx;
	float: right;
	margin: 29rpx;
}
.user .paging-img {
	width: 30rpx;
	height: 28rpx;
	margin: 32rpx 30rpx;

	float: left;
}
.user .paging-title {
	width: 300rpx;
	height: 88rpx;
	line-height: 88rpx;
	font-size: 30rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
}

