
.logo {
		width: 182rpx;
		height: 182rpx;
		margin: 0 auto;
		margin-top: 40rpx;
}
.logo>image {
		width: 100%;
		height: 100%;
}
.name {
		width: 100%;
		text-align: center;
		margin-top: 19rpx;
		height: 30rpx;
		font-size: 28rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(51, 51, 51, 1);
		line-height: 30rpx;
}
.title {
		width: 100%;
		height: 30rpx;
		font-size: 28rpx;
		text-align: center;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(102, 102, 102, 1);
		line-height: 30rpx;
		margin-top: 25rpx;
}
.input-view {
		width: 630rpx;
		overflow: hidden;
		margin: 0 auto;
		margin-top: 70rpx;
}
.view-inp {
		width: 100%;
		height: 106rpx;
		border-bottom: 1rpx solid rgba(230, 230, 230, 1);
}
.code {
		width: 131rpx;
		height: 34rpx;
		border: 1rpx solid rgba(201, 0, 9, 1);
		border-radius: 17rpx;
		text-align: center;
		line-height: 34rpx;
		font-size: 22rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(200, 0, 10, 1);
		float: right;
		margin-top: 37rpx;
}
.input {
		width: 400rpx;
		height: 106rpx;
		overflow: hidden;
}
.input>input {
		width: 400rpx;
		height: 106rpx;
		line-height: 106rpx;
		font-size: 28rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(153, 153, 153, 1);
}
.btn {
		width:631rpx;
height:80rpx;
background:rgba(200,0,10,1);
opacity:0.5;
