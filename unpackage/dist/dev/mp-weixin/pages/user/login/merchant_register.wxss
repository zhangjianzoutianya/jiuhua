
#app {
	background: rgba(238, 238, 238, 1);
	width: 100%;
	height: 100%;
}
.merchant {
	width: 100%;
	height: 105rpx;
	background-color: rgb(255, 255, 255);
	overflow: hidden;
	border-top: 1rpx solid rgba(228, 228, 228, 1);
}
.merchant-img {
	width: 65rpx;
	height: 65rpx;
	overflow: hidden;
	float: right;
	margin: 20rpx;
	margin-right: 59rpx;
	border-radius: 50%;
}
.merchant-img>image {
	width: 100%;
	height: 100%;
}
.merchant-text {
	line-height: 105rpx;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
	padding-left: 61rpx;
}
.detailed {
	width: 100%;
	overflow: hidden;
	background-color: rgb(255, 255, 255);
	margin-top: 20rpx;
}
.detailed-view {
	width: 630rpx;
	height: 108rpx;
	border-bottom: 1rpx solid rgba(230, 230, 230, 1);
	margin: 0 auto;
	overflow: hidden;
}
.detailed-view:last-child {
	border-bottom: 1rpx solid rgba(255, 255, 255, 1);
}
.detailed-view>image {
	width: 17rpx;
	height: 30rpx;
	margin-top: 38rpx;
	float: right;
}
.detailed-name {
	line-height: 108rpx;
	width: 200rpx;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
	float: left;
}
.detailed-input {
	width: 350rpx;
	height: 108rpx;
	overflow: hidden;
	line-height: 108rpx;
}
.detailed-input>input {
	height: 108rpx;
	line-height: 108rpx;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(153, 153, 153, 1);
}
.detailed-input>view {
	height: 108rpx;
	line-height: 108rpx;
	font-size: 28rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(153, 153, 153, 1);
}
.license {
	width: 100%;
	height: 106rpx;
	font-size: 32rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(51, 51, 51, 1);
	line-height: 106rpx;
	margin-left: 61rpx;
}
.license-img {
	width: 100%;
	background-color: rgba(255, 255, 255, 1);
	overflow: hidden;
}
.license-image {
	width: 150rpx;
	height: 150rpx;
	margin: 103rpx auto;
	overflow: hidden;
}
.license-image>image {
	width: 100%;
	height: 100%;
}
.submission {
	width: 631rpx;
	height: 80rpx;
	background: rgba(200, 0, 10, 1);
	opacity: 0.5;
	border-radius: 40rpx;
	margin: 0 auto;
	margin-bottom: 64rpx;
	text-align: center;
	line-height: 80rpx;
	font-size: 30rpx;
	font-family: PingFang-SC-Medium;
	font-weight: 500;
	color: rgba(255, 255, 255, 1);
}

