
#app {
		background-color: #F7F7F7;
		overflow: hidden;
		height: 100%;
}
.details {
		width: 699rpx;
		height: 804rpx;
		background: rgba(255, 255, 255, 1);
		border-radius: 10rpx;
		margin: 20rpx auto;
		position: relative;
}
.comm-info {
		width: 100%;
		height: 211rpx;
		border-bottom: 1rpx solid rgba(238, 238, 238, 1);
		overflow: hidden;
}
.comm-img {
		width: 170rpx;
		height: 145rpx;
		margin: 30rpx 25rpx;
		float: left;
		overflow: hidden;
		border-radius: 10rpx;
}
.comm-img>image {
		width: 100%;
		height: 100%;
}
.comm-title {
		width: 450rpx;
		height: 145rpx;
		margin-top: 30rpx;
		overflow: hidden;
}
.comm-text {
		width: 100%;
		height: 80rpx;
		line-height: 40rpx;
		overflow: hidden;
		font-size: 28rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(51, 51, 51, 1);
}
.comm-msg {
		width: 100%;
		height: 25rpx;
		margin-top: 35rpx;
		overflow: hidden;
}
.comm-data {
		font-size: 24rpx;
		line-height: 25rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(102, 102, 102, 1);
}
.comm-time {
		font-size: 24rpx;
		line-height: 25rpx;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		width: 50%;
		float: left;
		color: rgba(102, 102, 102, 1);
}
.erweima {
		width: 248rpx;
		height: 248rpx;
		border: 1rpx solid rgba(229, 229, 229, 1);
		margin: 0 auto;
		margin-top: 48rpx;
}
.erweima>image {
		width: 210rpx;
		height: 210rpx;
		margin: 19rpx;
}
.serial-number {
		width: 100%;
		height: 42rpx;
		font-size: 30rpx;
		text-align: center;
		font-family: PingFang-SC-Bold;
		font-weight: bold;
		color: rgba(51, 51, 51, 1);
		line-height: 42rpx;
		margin-top: 32rpx;
}
.count-down {
		height: 42rpx;
		font-size: 24rpx;
		text-align: center;
		font-family: PingFang-SC-Medium;
		font-weight: 500;
		color: rgba(51, 51, 51, 1);
		line-height: 42rpx;
}
.order {
		width: 100%;
		height: 143rpx;
		border-top:1rpx solid rgba(229,229,229,1);
		position: absolute;
		bottom: 0;
		left: 0;
}
.order-text {
		width: 100%;
		height: 26rpx;
		font-size:24rpx;
		line-height: 26rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(153,153,153,1);
margin-top: 31rpx;
padding-left: 20rpx;
}
